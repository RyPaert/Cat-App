@model Catblog.Models.Cats.CatImage

<div class="form-group row col-sm-4 offset-2">
    <div>
        @Model.ImageTitle
        @if (!Model.Image.Any())
        {
            <p>"No images are found"</p>
        }
    </div>
    <img width="256" class="card-img-top" src="@Model.Image" asp-append-version="true" />
</div>